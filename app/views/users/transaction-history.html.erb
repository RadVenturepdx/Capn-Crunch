<!-- Page Heading/Breadcrumbs -->
<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header" style="text-align:center">Transaction History</h1>
  </div>
</div>
<!-- /.row -->

<!-- Transaction Section -->
<div id="transactionHistory">
  <div class="row">
        <table class="table table-responsive">
          <!-- Transaction Heading-->
          <thread>
            <tr>
              <th>Date</th>
              <th>Location</th>
              <th>Price</th>
              <th>Guide</th>
              <th>Payment Status</th>
              <th>Review</th>
            </tr>
          </thread>

          <!-- transaction -->
          <thread>
            <tr>
              <td>2015/02/01 09:00</td>
              <td>Bachelor</td>
              <td>$200</td>
              <td>Jane Doe</td>
              <td class="text-success">
                <i class="fa fa-check "></i> Paid
              </td>
              <td>Completed</td>
            </tr>
          </thread>
          <!-- transaction -->
          <thread>
            <tr>
              <td>2015/02/10 13:30</td>
              <td>Hood</td>
              <td>$250</td>
              <td>Johnny Jones</td>
              <td class="text-warning">
                <i class="fa fa-exclamation-circle"></i> Pending
              </td>
              <td><%= link_to "Not Submitted", review_path %></td>
            </tr>
          </thread>
          <!-- transaction -->
          <thread>
            <tr>
              <td>2015/02/16 19:45</td>
              <td>Whistler</td>
              <td>$300</td>
              <td>Jeff Jefferson</td>
              <td class="text-danger">
                <i class="fa fa-minus"></i> Declined
              </td>
              <td><%= link_to "Not Submitted", review_path %></td>
            </tr>
          </thread>
        </table>

  </div>
</div>
<!-- /.row -->